{
    "b-badge-pill": {
        "prefix": "b-badge-pill",
        "body": "<span class=\"badge badge-pill badge-${1|primary,secondary,success,danger,warning,info,light,dark|}\">${2:Text}</span>"
    },
    "b-alert": {
        "prefix": "b-alert",
        "body": [
            "<div class=\"alert alert-${1|primary,secondary,success,danger,warning,info,light,dark|}\" role=\"alert\">",
            "\t${2:Content}",
            "</div>"
        ]
    }
}